DEPARTMENT VARCHAR(255) NOT NULL,
STATUS VARCHAR2(20) NOT NULL,
PHONENO	VARCHAR2(20) NOT NULL

)TABLESPACE NEW_DATA;



GRANT SELECT, INSERT, UPDATE, DELETE ON NEWSO.EMPLOYEE TO NEW_ROLE_CU_RW;

